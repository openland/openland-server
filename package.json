  "scripts": {
    "assets": "yarn cpx \"./src/assets/**/*\" ./build/assets/",
    "build": "yarn assets && ./node_modules/.bin/tsc -p tsconfig.json",
    "build:watch": "yarn assets && ./node_modules/.bin/tsc -w -p tsconfig.json",
    "start:watch": "nodemon build/index.js",
    "start:watch:inspect": "nodemon --inspect build/index.js",
    "start": "BLUEBIRD_LONG_STACK_TRACES=0 node ./build/index.js",
    "dev": "yarn build && ELASTIC_ENDPOINT=\"http://localhost:9200/\" REDIS_HOST=\"localhost\" ./node_modules/.bin/concurrently --kill-others \"yarn build:watch\" \"yarn start:watch\"",
    "dev:inspect": "BLUEBIRD_LONG_STACK_TRACES=0 yarn build && ./node_modules/.bin/concurrently --kill-others \"yarn build:watch\" \"yarn start:watch:inspect\"",
    "dev:db": "RECREATE_DB=true yarn build && ./node_modules/.bin/concurrently --kill-others \"yarn build:watch\" \"yarn start:watch\"",
    "heroku-postbuild": "yarn build",
    "promote": "kubectl set image deployment/statecraft-server statecraft-server=`kubectl get deployment statecraft-server-staging -o=jsonpath='{$.spec.template.spec.containers[1:2].image}'` && kubectl rollout status deployments statecraft-server"
  },
  "devDependencies": {}
}
