  "scripts": {
    "geoIP": "yarn cpx \"./packages/openland-server/utils/geoIp/*.json\" ./build/openland-server/utils/geoIp/",
    "assets": "yarn cpx \"./packages/openland-server/assets/**/*\" ./build/openland-server/assets/",
    "assets:watch": "yarn cpx \"./packages/openland-server/assets/**/*\" ./build/openland-server/assets/ --watch",
    "schema": "yarn cpx \"./packages/openland-server/**/*.graphql\" ./build/openland-server/",
    "schema:watch": "yarn cpx \"./packages/openland-server/**/*.graphql\" ./build/openland-server/ --watch",
    "build": "yarn schema && yarn assets && yarn geoIP && ./node_modules/.bin/tsc -p tsconfig.json",
    "build:watch": "./node_modules/.bin/tsc -w -p tsconfig.json",
    "start:watch": "nodemon build/openland-server/index.js",
    "start:watch:inspect": "nodemon --inspect build/openland-server/index.js",
    "start": "BLUEBIRD_LONG_STACK_TRACES=0 node ./build/openland-server/index.js",
    "dev": "source ./scripts/set_dev_config.sh && yarn build && ./node_modules/.bin/concurrently --kill-others \"yarn schema:watch\" \"yarn assets:watch\" \"yarn build:watch\" \"yarn start:watch\"",
    "dev:inspect": "source ./scripts/set_dev_config.sh && yarn build && BLUEBIRD_LONG_STACK_TRACES=0 node --inspect ./build/index.js",
    "dev:db": "source ./scripts/set_dev_config.sh && yarn build && RECREATE_DB=true yarn start",
    "promote": "kubectl set image deployment/statecraft-server statecraft-server=`kubectl get deployment statecraft-server-staging -o=jsonpath='{$.spec.template.spec.containers[1:2].image}'` && kubectl rollout status deployments statecraft-server",
    "lint": "NODE_ENV=production ./node_modules/.bin/tslint --project . --config ./tslint.json",
    "test:watch": ". ./scripts/set_testing_config.sh && node ./node_modules/jest/bin/jest.js --watch --verbose",
    "test": ". ./scripts/set_testing_config.sh && node --no-deprecation ./node_modules/jest/bin/jest.js --forceExit --runInBand --coverage",
    "test:clean": "./scripts/testing_clean.sh",
    "test:start": "./scripts/testing_start.sh",
    "test:stop": "./scripts/testing_stop.sh",
    "test:prepare": ". ./scripts/set_testing_config.sh && DATABASE_LOGGING=true node ./build/index.js --rebuild-test",
    "test:integration": "yarn test:jest",
    "test:rebuild": "yarn test:start && yarn test:prepare && yarn test:stop",
    "schema:publish": "apollo schema:publish --key=\"service:openland-production:qrYALC8Flb8hv-SGm4grug\" --endpoint=\"https://api.openland.com/api\"",
    "model": "./node_modules/.bin/ts-node packages/foundation-schema/index.ts"
  },
  "devDependencies": {},
  "_moduleAliases": {
    "foundation-orm": "build/foundation-orm",
    "openland-server": "build/openland-server",
    "openland-migrations": "build/openland-migrations"
  }
