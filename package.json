  "scripts": {
    "assets": "yarn cpx \"./packages/openland-server/assets/**/*\" ./build/openland-server/assets/",
    "assets:watch": "yarn cpx \"./packages/openland-server/assets/**/*\" ./build/openland-server/assets/ --watch",
    "schema": "yarn cpx \"./packages/**/*.graphql\" ./build/",
    "schema:watch": "yarn cpx \"./packages/**/*.graphql\" ./build/ --watch",
    "build": "yarn schema && yarn assets && cpx ./packages/openland-module-db/structs.js ./build/openland-module-db/ && npx tsc -p tsconfig.json && yarn vostok-schema:copy",
    "build:watch": "./node_modules/.bin/tsc -w -p tsconfig.json",
    "start:watch": "nodemon build/openland-server/index.js",
    "start:inspect": "nodemon --inspect build/openland-server/index.js",
    "start": "node ./build/openland-server/index.js",
    "dev": ". ./scripts/set_dev_config.sh && yarn build && ./node_modules/.bin/concurrently --kill-others \"yarn schema:watch\" \"yarn assets:watch\" \"yarn build:watch\" \"yarn start:watch\" \"yarn schema:gen-watch\" \"yarn vostok-proto:watch\" \"yarn vostok-api:watch\"",
    "dev:unwatch": ". ./scripts/set_dev_config.sh && yarn build && yarn start",
    "dev:inspect": ". ./scripts/set_dev_config.sh && yarn build && node --inspect --trace_gc --nouse-idle-notification --max-semi-space-size=128 --max_old_space_size=1024 ./build/openland-server/index.js",
    "dev:db": ". ./scripts/set_dev_config.sh && yarn build && RECREATE_DB=true yarn start",
    "dev:reset-db": "./node_modules/.bin/ts-node packages/openland-server-tests/prepare.ts",
    "dev:init": ". ./scripts/set_dev_config.sh && ./node_modules/.bin/ts-node packages/openland-server-tests/generate_fake_activity.ts",
    "lint": "NODE_ENV=production ./node_modules/.bin/tslint --project . --config ./tslint.json",
    "test:watch": ". ./scripts/set_testing_config.sh && node ./node_modules/jest/bin/jest.js --watch --verbose",
    "test": ". ./scripts/set_testing_config.sh && node --no-deprecation ./node_modules/jest/bin/jest.js --forceExit --",
    "test:fdb": ". ./scripts/set_testing_config.sh && node --no-deprecation ./node_modules/jest/bin/jest.js --forceExit packages/foundation-orm --",
    "test:coverage": ". ./scripts/set_testing_config.sh && node --no-deprecation ./node_modules/jest/bin/jest.js --forceExit --coverage",
    "schema:publish": "apollo schema:publish --key=\"service:openland-production:qrYALC8Flb8hv-SGm4grug\" --endpoint=\"https://api.openland.com/api\"",
    "schema:gen": "./node_modules/.bin/ts-node packages/openland-module-api/schema/SchemaGenSpec.ts",
    "schema:gen-watch": "nodemon -w \"./packages\" -e graphql --exec \"yarn schema:gen\"",
    "schema:analyze": "./node_modules/.bin/ts-node packages/openland-module-api/schema/usageAnalyzer.ts",
    "vostok-proto:gen": "./node_modules/.bin/pbjs -t static-module ./packages/openland-mtproto3/vostok/vostok.proto -o ./packages/openland-mtproto3/vostok/schema/schema.js -w commonjs && ./node_modules/.bin/pbts  ./packages/openland-mtproto3/vostok/schema/schema.js -o ./packages/openland-mtproto3/vostok/schema/schema.d.ts",
    "vostok-proto:watch": "nodemon -w \"./packages/openland-mtproto3/vostok/schema\" -e js --exec \"yarn vostok-schema:copy\"",
    "vostok-api:gen": "./node_modules/.bin/pbjs -t static-module ./packages/openland-mtproto3/vostok-api/vostok-api.proto -o ./packages/openland-mtproto3/vostok-api/schema/schema.js -w commonjs && ./node_modules/.bin/pbts  ./packages/openland-mtproto3/vostok-api/schema/schema.js -o ./packages/openland-mtproto3/vostok-api/schema/schema.d.ts",
    "vostok-api:watch": "nodemon -w \"./packages/openland-mtproto3/vostok-api/schema\" -e js --exec \"yarn vostok-schema:copy\"",
    "vostok-schema:copy": "cpx \"./packages/openland-mtproto3/vostok/schema/schema.js\" ./build/openland-mtproto3/vostok/schema/ && cpx \"./packages/openland-mtproto3/vostok-api/schema/schema.js\" ./build/openland-mtproto3/vostok-api/schema/",
    "model": "yarn model:new",
    "model:new": "./node_modules/.bin/ts-node ./packages/openland-module-db/store.compile.ts",
    "model:old": "./node_modules/.bin/ts-node packages/openland-module-db/schemaDeclaration.ts",
    "model:test": "./node_modules/.bin/ts-node ./packages/foundation-orm/tests/createTestSchema.ts",
    "benchmark": "ts-node ./packages/openland-module-db/collections/algs/Algorithm.benchmark.ts",
    "proto": "pbjs -t static-module -w commonjs -o ./packages/openland-module-db/structs.js ./packages/openland-module-db/structs.proto && pbts -o ./packages/openland-module-db/structs.d.ts ./packages/openland-module-db/structs.js",
    "cli": ". ./scripts/set_testing_config.sh && ./node_modules/.bin/ts-node ./packages/openland-cli/index.ts",
    "ch:gen": ". ./scripts/set_dev_config.sh && ./node_modules/.bin/ts-node packages/openland-module-clickhouse/generateMigrations.ts"
  },
